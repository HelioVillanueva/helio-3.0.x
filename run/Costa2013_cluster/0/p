FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    BACK
    {
        type            symmetry;
    }
    FRONT
    {
        type            symmetry;
    }
    FUELIN
    {
        type            zeroGradient;
    }
    OUTLET
    {
        type            totalPressure;
        rho             none;
        psi             none;
        gamma           1.4;
        p0              uniform 0;
        value           uniform 0;
    }
    OXIN
    {
        type            zeroGradient;
    }
    WALL
    {
        type            zeroGradient;
    }
}


